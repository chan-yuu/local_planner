<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- 启动gazebo并加载world文件，这里假设你的world文件名为example.world，放在了某个特定目录下，根据实际调整路径 -->
    <!-- <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find your_package_name)/worlds/example.world"/>
    </include> -->

    <!-- 启动rviz用于可视化 -->
    <!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find your_package_name)/rviz/config.rviz"/> -->
    
    
    <node name="obstacle_perception" pkg="obstacle_perception" type="obstacle_perception_node"/>

    <!-- 启动tf广播，用于发布world和map坐标系之间的静态坐标变换 -->
    <node name="static_tf_publisher" pkg="tf2_ros" type="static_transform_publisher" args="0 0 0 0 0 0 /rslidar /vehicle_frame"/>

</launch>